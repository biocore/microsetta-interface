            updateCompare(state);
            updateDiversity(state);
            updateSimilarity(state);
            updateTaxonomy(taxonomyData, state);
            updateDatasetDetails(state);

            //buildMicrobialMapsPlots(state);
            getMicrobiomeMapsRasterizedPlots(state);
        }, state.dataset_input);

        refreshDatasets(state);
    }

    $(document).ready(init);
</script>
{% endblock %}

